[alias]
	lol = log --graph --decorate --pretty=oneline --abbrev-commit
	lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
	wdiff = diff --color-words
[color]
	branch = auto
	diff = auto
	interactive = auto
	status = auto
[merge]
	conflictStyle = diff3
[pull]
	rebase = true
[rebase]
	autostash = true
[core]
	excludesfile = ~/.gitignore_global
